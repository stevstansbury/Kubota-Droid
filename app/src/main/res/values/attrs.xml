<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GaugeView">
        <attr name="text" format="string|reference" />
        <attr name="percent" format="float|reference" />
        <attr name="gaugeType" format="enum">
            <enum name="DEF" value="0" />
            <enum name="Fuel" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="BatteryIndicatorView">
        <attr name="voltage" format="float|reference" />
    </declare-styleable>

    <declare-styleable name="MachineCardView">
        <attr name="viewType" format="enum">
            <enum name="list" value="0" />
            <enum name="cab" value="1" />
            <enum name="detail" value="2" />
            <enum name="edit" value="3" />
        </attr>
    </declare-styleable>
    <declare-styleable name="KubotaSwitchPreference">
        <attr name="title" format="string" />
        <attr name="summary" format="string" />
    </declare-styleable>
</resources>
